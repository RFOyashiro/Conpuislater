%include "io.asm"
section .bss
sinput resb 255
$a resw 1
$b resw 1
section .text
global _start
_start:
call main
mov eax,1
int 0x80
main:
mov eax, sinput
call readline
call atoi
push eax
pop ebx
mov [$a], ebx
push word[$a]
push word[$a]
pop ebx
pop eax
mul ebx
push eax
pop ebx
mov [$b], ebx
push word[$b]
pop eax
call iprintLF
ret
